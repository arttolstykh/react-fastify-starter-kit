{
  "name": "react-fasify-starter-kit",
  "version": "1.0.0",
  "description": "Full-stack Boilerplate",
  "main": "index.js",
  "moduleRoots": [
    "app/client",
    "app/server",
    "app/common",
    "app",
    "node_modules"
  ],
  "engines": {
    "node": ">=15.0.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/prod.config.js",
    "start:server": "cross-env NODE_ENV=development webpack --config ./webpack/server.config.js",
    "start:client": "cross-env NODE_ENV=development webpack serve --config ./webpack/client.config.js",
    "start": "npm run build && node ./dist/server.js",
    "develop": "run-p start:client start:server",
    "test": "standard"
  },
  "author": "Artem Tolstykh",
  "license": "ISC",
  "dependencies": {
    "fastify": "^3.8.0",
    "fastify-cors": "^4.1.0",
    "fastify-static": "^3.3.0",
    "history": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "babel-loader": "^8.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.2",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "html-webpack-plugin": "^5.0.0-alpha.14",
    "mini-css-extract-plugin": "^1.3.1",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "razzle-start-server-webpack-plugin": "^4.0.0-canary.3",
    "sass-loader": "^10.1.0",
    "standard": "^16.0.3",
    "style-loader": "^2.0.0",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.4.0",
    "webpack-node-externals": "^2.5.2"
  }
}
