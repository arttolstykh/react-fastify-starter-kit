{
  "name": "react-fasify-starter-kit",
  "version": "1.0.0",
  "description": "Full-stack Boilerplate",
  "main": "index.js",
  "moduleRoots": [
    "app/client",
    "app/server",
    "app/common",
    "app",
    "node_modules"
  ],
  "engines": {
    "node": ">=15.0.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production rm -rf ./dist && webpack --config ./webpack/prod.config.js",
    "start:client": "cross-env NODE_ENV=development webpack serve --config ./webpack/client/dev.config.js",
    "start:server": "cross-env NODE_ENV=development webpack --config ./webpack/server/dev.config.js",
    "start": "npm run build && node ./dist/server.js",
    "develop": "run-p start:client start:server",
    "test": "standard"
  },
  "author": "Artem Tolstykh aka Mr. Arch",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "*",
    "@material-ui/icons": "*",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/styles": "*",
    "clsx": "*",
    "connected-react-router": "*",
    "fastify": "*",
    "fastify-cors": "*",
    "fastify-plugin": "*",
    "fastify-static": "*",
    "history": "*",
    "primus": "*",
    "primus-emit": "*",
    "prop-types": "*",
    "react": "*",
    "react-dom": "*",
    "react-redux": "*",
    "react-router-dom": "*",
    "redux": "*",
    "redux-logger": "*",
    "terser": "*"
  },
  "devDependencies": {
    "@babel/cli": "*",
    "@babel/core": "*",
    "@babel/plugin-proposal-class-properties": "*",
    "@babel/plugin-proposal-object-rest-spread": "*",
    "@babel/plugin-transform-runtime": "*",
    "@babel/preset-env": "*",
    "@babel/preset-react": "*",
    "babel-loader": "*",
    "clean-webpack-plugin": "*",
    "copy-webpack-plugin": "*",
    "cross-env": "*",
    "css-loader": "*",
    "css-minimizer-webpack-plugin": "*",
    "html-webpack-plugin": "^5.0.0-alpha.14",
    "mini-css-extract-plugin": "*",
    "node-sass": "*",
    "npm-run-all": "*",
    "postcss-loader": "*",
    "postcss-preset-env": "*",
    "razzle-start-server-webpack-plugin": "^4.0.0-canary.3",
    "sass-loader": "*",
    "standard": "*",
    "style-loader": "*",
    "webpack": "*",
    "webpack-cli": "*",
    "webpack-dev-server": "*",
    "webpack-merge": "*",
    "webpack-node-externals": "*"
  }
}
