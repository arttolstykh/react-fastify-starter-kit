{
  "name": "react-fasify-starter-kit",
  "version": "1.0.0",
  "description": "Full-stack Boilerplate",
  "main": "index.js",
  "moduleRoots": [
    "app/client",
    "app/server",
    "app/common",
    "app",
    "node_modules"
  ],
  "engines": {
    "node": ">=15.0.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production rm -rf ./dist && webpack --config ./webpack/prod.config.js",
    "start:client": "cross-env NODE_ENV=development webpack serve --config ./webpack/client/dev.config.js",
    "start:server": "cross-env NODE_ENV=development webpack --config ./webpack/server/dev.config.js",
    "start": "npm run build && node ./dist/server.js",
    "develop": "run-p start:client start:server",
    "test": "standard"
  },
  "author": "Artem Tolstykh aka Mr. Arch",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/styles": "^4.11.1",
    "clsx": "^1.1.1",
    "fastify": "^3.8.0",
    "fastify-cors": "^5.0.0",
    "fastify-plugin": "^3.0.0",
    "fastify-static": "^3.3.0",
    "history": "^4.9.0",
    "primus": "^8.0.0",
    "primus-emit": "^1.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "terser": "^5.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "babel-loader": "^8.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.1",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "html-webpack-plugin": "^5.0.0-alpha.14",
    "mini-css-extract-plugin": "^1.3.1",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^4.0.4",
    "postcss-preset-env": "^6.7.0",
    "razzle-start-server-webpack-plugin": "^4.0.0-canary.3",
    "sass-loader": "^10.1.0",
    "standard": "^16.0.2",
    "style-loader": "^2.0.0",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.4.0",
    "webpack-node-externals": "^2.5.2"
  }
}
